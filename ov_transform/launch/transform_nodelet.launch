<launch>

    <!-- what config we are going to run (should match folder name) -->
   
    <arg name="nodelet_manager_name" default="ovmsckf_nodelet_manager"/>
    <arg name="start_nodelet_manager" default="false"/>
    <arg name="config"      default="rs435i" /> <!-- euroc_mav, tum_vi, rpng_aruco -->
    <arg name="config_path" default="$(find ov_msckf)/../config/$(arg config)/estimator_config.yaml" />
    



  <node pkg="nodelet"
      type="nodelet"
      name="$(arg nodelet_manager_name)"
      args="manager"
      output="screen"
      if="$(arg start_nodelet_manager)"/>
	
 <node pkg="nodelet"
      type="nodelet"
      args="load transform_nodelet/OvtransformNodeletClass /$(arg nodelet_manager_name)"
      name="transform_nodelet" 
      output="screen">
      <param name="config_path"            type="string" value="$(arg config_path)" />
    </node>
    

</launch>
